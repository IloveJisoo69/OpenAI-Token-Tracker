{
  "manifest_version": 3,
  "name": "Token Tracker",
  "version": "1.0",
  "description": "Counts tokens and estimates cost in ChatGPT prompts.",
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["https://chatgpt.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}
